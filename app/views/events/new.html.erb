<%= form_with(model: @event, local: true) do |f| %>
  <p>
    <%= f.label :location %>
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :event_date %>
    <%= f.date_field :event_date %>
  </p>
  <%= f.submit %>
<% end %>
